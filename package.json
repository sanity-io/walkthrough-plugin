{
  "name": "sanity-plugin-walkthrough",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "license": "UNLICENSED",
  "author": "Drew Lyton <drew.lyton@sanity.io>",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "dev": "concurrently \"npm run link-watch\" \"npm run css:watch\"",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "css:watch": "npx tailwindcss -i ./src/tailwind.css -o ./src/index.css --watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@portabletext/react": "^3.0.11",
    "@sanity/incompatible-plugin": "^1.0.4",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@sanity/icons": "^2.10.2",
    "@sanity/pkg-utils": "^4.2.4",
    "@sanity/plugin-kit": "^3.1.10",
    "@sanity/ui": "^2.0.3",
    "@types/react": "^18.2.57",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.5",
    "sanity": "^3.30.0",
    "styled-components": "^5.3.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@sanity/icons": "^2",
    "@sanity/ui": "^2",
    "@sanity/telemetry": "^0.7.7",
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  }
}
